{
  "name": "training-doctor-api-on-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/server.ts",
    "dev": "nodemon ./src/server.ts",
    "test": "jest --no-cache --verbose --silent --detectOpenHandles --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "mongoose": "^5.10.18",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "typescript": "^4.1.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
