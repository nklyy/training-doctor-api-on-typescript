{
  "name": "training-doctor-api-on-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "start:build": "node ./dist/server",
    "postinstall": "tsc",
    "dev": "nodemon ./src/server.ts",
    "test": "jest --no-cache --verbose --silent --detectOpenHandles --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "mongoose": "^5.11.17",
    "nodemon": "^2.0.22",
    "supertest": "^6.1.3",
    "typescript": "^4.1.5",
    "winston": "^3.3.3",
    "eslint": "^7.20.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1"
  }
}
